<%- include("partials/header", {apiKey : apiKey}) %>

<h1>Compose</h1>
<form action="/compose" method="POST">
  <div class="form-group">
    <label>Title</label>
    <input type="text" class="form-control" placeholder="Blog Title" name="postTitle" autocomplete="off">
  </div>
  <div class="form-group">
    <label>Post</label>
    <textarea class="form-control" id="mytextarea" rows="13" name="postContent"></textarea>
  </div>
  <button type="submit" class="btn btn-primary active" name="button" value="compose">Submit</button>
</form>

<%- include("partials/footer") %>